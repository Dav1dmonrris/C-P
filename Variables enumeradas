//variables_enumeradas.h
#include <stdio.h>
#include <locale.h>


int main(int argc, char const *argv[])
{
    setlocale(LC_ALL, "spanish");
    
    enum colores {verde, rojo=10, azul};

    printf("El color verde es: %d, %i, %d\n", verde, rojo, azul );
   

    //Dar valores con el codigo ASCII
    int numero1, numero2, total;


    numero1= 'A';
    numero2= 5;

    total= 5 + 'A';

    printf("el valor de %d + %d = %d \n", numero1, numero2, total);







    getch();
    return 0;
}
