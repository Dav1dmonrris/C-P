//Circuitos_paralelo.h
//Metodologia
#include <stdio.h>
#include <locale.h>

int main(int argc, char const *argv[])
{

    setlocale(LC_ALL, "spanish");
    int numero, ohms, volt;
    int suma=0; 

    printf("Escriba las resistencias que hay en el 1er circuito\n");

    while(1){
        if (scanf("%d", &ohms)==1)
        {
           suma+=ohms; 
        }
        else {
            break;
        }
    }
    printf("C1=\n");
    printf("La suma es %d\n", suma);





    return 0;
}
