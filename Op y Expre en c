#include <stdio.h>
#include <locale.h>

int main(int argc, char const *argv[])
{
	setlocale(LC_ALL, "spanish");
int a, b, c, resultado;
	printf("Asignacion simple:\n");

	a=10;
	printf("el valor de a es %d\n", a);

	a=15;
	printf("el valor de a es %d\n", a);

	a=20;
	printf("el valor de a es %d\n", a);
     
    a=40;
	printf("el valor de a es %d\n", a);

	printf("Asignacion multiple:\n");

    a=b=c=10;
	printf("el valor de a es %d\n", a);


    printf("Operadores Aritmeticos:\n");
    printf("Donde a=8; y b=4; \n\n");

    a=8;
    b=4;
    resultado=a+b;

    printf("El resultado de %d+%d=%d\n", a, b, resultado);


    a=8;
    b=4;
    resultado=a-b;

    printf("El resultado de %d-%d=%d\n", a, b, resultado);

    a=8;
    b=4;
    resultado=a/b;

    printf("El resultado de %d/%d=%d\n", a, b, resultado);

    a=8;
    b=4;
    resultado=a*b;

    printf("El resultado de %d*%d=%d\n", a, b, resultado);

    //Modulo

    a=8;
    b=4;
    resultado=a*b;
    printf("El resultado de %d%%%d=%d\n", a, b, resultado);






	getch();
	return 0;
}
